<Window x:Class="Sela.Samples.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <StackPanel>
            <TextBlock Text="{Binding Data}" FontSize="32"/>
            <Button Content="Result" Command="{Binding}" CommandParameter="R" FontSize="32" Margin="5"/>
            <Button Content="Async" Command="{Binding}" CommandParameter="A" FontSize="32" Margin="5"/>
        </StackPanel>
    </Grid>
</Window>
